{"ast":null,"code":"import { Chart, registerables } from 'chart.js';\nChart.register(...registerables);\nexport default {\n  name: 'Dashboard',\n  data() {\n    return {\n      cards: [{\n        title: '資產總數',\n        count: 1200,\n        color: 'primary'\n      }, {\n        title: '合規數',\n        count: 800,\n        color: 'success'\n      }, {\n        title: '未合規數',\n        count: 300,\n        color: 'danger'\n      }, {\n        title: '未稽核數',\n        count: 100,\n        color: 'secondary'\n      }]\n    };\n  },\n  mounted() {\n    this.initCharts();\n  },\n  methods: {\n    initCharts() {\n      const ctxCompliance = this.$refs.complianceChart.getContext('2d');\n      const complianceChart = new Chart(ctxCompliance, {\n        type: 'doughnut',\n        data: {\n          labels: ['Passed', 'Failed', 'Other'],\n          datasets: [{\n            data: [800, 300, 100],\n            backgroundColor: ['#4caf50', '#f44336', '#ffeb3b'],\n            hoverBackgroundColor: ['#66bb6a', '#ef5350', '#fdd835']\n          }]\n        }\n      });\n      const ctxAudit = this.$refs.auditChart.getContext('2d');\n      const auditChart = new Chart(ctxAudit, {\n        type: 'bar',\n        data: {\n          labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May'],\n          datasets: [{\n            label: 'Audit Count',\n            data: [50, 75, 150, 125, 225],\n            backgroundColor: '#2196f3'\n          }]\n        }\n      });\n\n      // Add additional chart initialization here as needed\n    }\n  }\n};","map":{"version":3,"names":["Chart","registerables","register","name","data","cards","title","count","color","mounted","initCharts","methods","ctxCompliance","$refs","complianceChart","getContext","type","labels","datasets","backgroundColor","hoverBackgroundColor","ctxAudit","auditChart","label"],"sources":["C:\\Users\\2409\\Desktop\\vue3最新\\vue3\\src\\views\\About.vue"],"sourcesContent":["<template>\r\n  <div class=\"container py-4\">\r\n    <!-- Header Cards -->\r\n    <div class=\"row text-center section bg-section-alt\">\r\n      <div class=\"col-md-3 mb-4\" v-for=\"(item, index) in cards\" :key=\"index\">\r\n        <div class=\"card h-100\" :class=\"'border-' + item.color\">\r\n          <div class=\"card-body\">\r\n            <h5 class=\"card-title\" :class=\"'text-' + item.color\">{{ item.title }}</h5>\r\n            <p class=\"card-text\">{{ item.count }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Compliance and Audit Overview Section -->\r\n    <div class=\"row section bg-section\">\r\n      <div class=\"col-md-6\">\r\n        <h4>資產合規率</h4>\r\n        <canvas ref=\"complianceChart\"></canvas>\r\n      </div>\r\n      <div class=\"col-md-6\">\r\n        <h4>近期稽核情形 (稽核資產數)</h4>\r\n        <canvas ref=\"auditChart\"></canvas>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Detailed Audit Statistics -->\r\n    <div class=\"row section bg-section-alt\">\r\n      <div class=\"col-md-6\">\r\n        <h4>Recent Audit Activities</h4>\r\n        <canvas ref=\"recentAuditChart\"></canvas>\r\n      </div>\r\n      <div class=\"col-md-6\">\r\n        <h4>Asset Compliance Rate</h4>\r\n        <canvas ref=\"assetComplianceRateChart\"></canvas>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { Chart, registerables } from 'chart.js';\r\nChart.register(...registerables);\r\n\r\nexport default {\r\n  name: 'Dashboard',\r\n  data() {\r\n    return {\r\n      cards: [\r\n        { title: '資產總數', count: 1200, color: 'primary' },\r\n        { title: '合規數', count: 800, color: 'success' },\r\n        { title: '未合規數', count: 300, color: 'danger' },\r\n        { title: '未稽核數', count: 100, color: 'secondary' }\r\n      ]\r\n    };\r\n  },\r\n  mounted() {\r\n    this.initCharts();\r\n  },\r\n  methods: {\r\n    initCharts() {\r\n      const ctxCompliance = this.$refs.complianceChart.getContext('2d');\r\n      const complianceChart = new Chart(ctxCompliance, {\r\n        type: 'doughnut',\r\n        data: {\r\n          labels: ['Passed', 'Failed', 'Other'],\r\n          datasets: [{\r\n            data: [800, 300, 100],\r\n            backgroundColor: ['#4caf50', '#f44336', '#ffeb3b'],\r\n            hoverBackgroundColor: ['#66bb6a', '#ef5350', '#fdd835']\r\n          }]\r\n        }\r\n      });\r\n\r\n      const ctxAudit = this.$refs.auditChart.getContext('2d');\r\n      const auditChart = new Chart(ctxAudit, {\r\n        type: 'bar',\r\n        data: {\r\n          labels: ['Jan', 'Feb', 'Mar', 'Apr', 'May'],\r\n          datasets: [{\r\n            label: 'Audit Count',\r\n            data: [50, 75, 150, 125, 225],\r\n            backgroundColor: '#2196f3'\r\n          }]\r\n        }\r\n      });\r\n\r\n      // Add additional chart initialization here as needed\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.section {\r\n  padding: 20px;\r\n  margin-bottom: 20px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 6px 12px rgba(0,0,0,0.1);\r\n}\r\n.bg-section {\r\n  background-color: #e9ecef;\r\n}\r\n.bg-section-alt {\r\n  background-color: #fff;\r\n}\r\n.card {\r\n  box-shadow: 0 0.125rem 0.25rem rgba(0,0,0,0.075);\r\n}\r\n.card h5 {\r\n  font-size: 1.25rem;\r\n}\r\n</style>\r\n"],"mappings":"AAyCA,SAASA,KAAK,EAAEC,aAAY,QAAS,UAAU;AAC/CD,KAAK,CAACE,QAAQ,CAAC,GAAGD,aAAa,CAAC;AAEhC,eAAe;EACbE,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE,IAAI;QAAEC,KAAK,EAAE;MAAU,CAAC,EAChD;QAAEF,KAAK,EAAE,KAAK;QAAEC,KAAK,EAAE,GAAG;QAAEC,KAAK,EAAE;MAAU,CAAC,EAC9C;QAAEF,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE,GAAG;QAAEC,KAAK,EAAE;MAAS,CAAC,EAC9C;QAAEF,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE,GAAG;QAAEC,KAAK,EAAE;MAAY;IAEpD,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,UAAU,EAAE;EACnB,CAAC;EACDC,OAAO,EAAE;IACPD,UAAUA,CAAA,EAAG;MACX,MAAME,aAAY,GAAI,IAAI,CAACC,KAAK,CAACC,eAAe,CAACC,UAAU,CAAC,IAAI,CAAC;MACjE,MAAMD,eAAc,GAAI,IAAId,KAAK,CAACY,aAAa,EAAE;QAC/CI,IAAI,EAAE,UAAU;QAChBZ,IAAI,EAAE;